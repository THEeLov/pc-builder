{
    "compilerOptions": {
        "jsx": "react-jsx",
        "allowJs": false,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "types": [
            "vite/client",
            "vitest"
        ],
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@/hooks/*": [
                "src/hooks/*"
            ],
            "@/data/*": [
                "src/data/*"
            ]
        }
    },
    "files": [],
    "include": [],
    "references": [
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.spec.json"
        }
    ],
    "extends": "../../tsconfig.base.json"
}
